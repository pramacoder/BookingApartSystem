# Supabase Configuration
# Dapatkan URL dan Anon Key dari dashboard Supabase Anda: https://app.supabase.com
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Firebase Configuration
# Dapatkan credentials dari Firebase Console: https://console.firebase.google.com
# Project Settings → General → Your apps → Web app config
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Midtrans Configuration (Optional - for payment integration)
# Dapatkan credentials dari Midtrans Dashboard: https://dashboard.midtrans.com
VITE_MIDTRANS_CLIENT_KEY=your_midtrans_client_key
VITE_MIDTRANS_SERVER_KEY=your_midtrans_server_key

